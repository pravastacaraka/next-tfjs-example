    "@tensorflow/tfjs": "4.7.0",
    "@vercel/analytics": "1.0.1",
    "framer-motion": "10.12.16",
    "next": "13.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "4.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.3",
    "@commitlint/config-conventional": "17.6.3",
    "encoding": "0.1.13",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.2",
    "eslint-config-prettier": "8.8.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "next-sitemap": "4.0.9",
    "prettier": "2.8.8"
